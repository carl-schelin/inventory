#!/bin/ksh

# this is the script used to manually process email'd changelogs that failed to process

for i in `ls *@*`
do
  EMAIL="`echo $i | cut -f1 -d@`@incojs01.scc911.com"
  RANDPASS=`echo $i | cut -f5 -d.`
  SUBJECT=`grep Subject: $i | cut -f2 -d\ `

  if [[ $EMAIL = 'jjohnson@incojs01.scc911.com' ]]
  then
    /usr/local/httpd/bin/changelog.submit.php $EMAIL $SUBJECT $RANDPASS
#    echo "/usr/local/httpd/bin/changelog.submit.php $EMAIL $SUBJECT $RANDPASS"
  fi
done

